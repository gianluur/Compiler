fn null empty() {
}

# Basic return types
fn int return_int() {
    return 42;
}

fn float return_float() {
    return 3.14;
}

fn bool return_bool() {
    return true;
}

# Parameters
fn null single_param(int x) {
}

fn int multiple_params(int x, float64 y, char z) {
    return x;
}

# Nested function calls
fn int nested() {
    var int x = return_int();
    return x + return_int();
}