// For with break
for (var int i = 0; i < 10; i = i + 1;) {
    if (i == 5) {
        break;
    }
}

// For with continue
for (var int i = 0; i < 10; i = i + 1;) {
    if (i == 5) {
        continue;
    }
}

// Nested for
for (var int i = 0; i < 5; i = i + 1;) {
    for (var int j = 0; j < 5; j = j + 1;) {
        var int x = i + j;
    }
}